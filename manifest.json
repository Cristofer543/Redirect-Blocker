{
  "manifest_version": 3,
  "name": "Redirect Stopper",
  "version": "1.0.0",
  "background": {
    "service_worker": "./src/background.js"
  },
  "icons": {
    "16": "./img/Icon16.png",
    "32": "./img/Icon32.png",
    "128": "./img/Icon128.png"
  },
  "action": {
    "default_icon": "./img/Icon128.png",
    "default_popup": "/src/popup/popup.html",
    "default_title": "Redirect Stopper"
  },
  "permissions": ["tabs", "activeTab", "storage"]
}
